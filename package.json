{
  "name": "todo-calendar-app",
  "version": "1.0.0",
  "description": "캘린더 일정 관리 및 팀 채팅 애플리케이션",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm --filter backend dev\" \"pnpm --filter frontend dev\"",
